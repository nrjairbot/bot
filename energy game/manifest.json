{
  "manifest_version": 2,

  "name": "Energy ClickClack",
  "description": "ClickClack energy",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["http://game.energy.ch/*"],
      "js": ["myscript.js"]
    }
  ]
}
