{
  "manifest_version": 2,

  "name": "Energy ClickClack",
  "description": "ClickClack energy",
  "version": "3.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://game.energy.ch/*","https://game.energy.ch/*"],
      "js": ["myscript.js"]
    }
  ]
}
